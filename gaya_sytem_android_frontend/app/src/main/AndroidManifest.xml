    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PembukuanFragmentChildView" />
        <activity android:name=".PembukuanFragmentChildAdd" />
        <activity android:name=".MasukMotorBaru"></activity>
        <activity android:name=".AbsenMotor"></activity>
        <activity android:name=".ViewMotor"></activity>
    </application>

</manifest>